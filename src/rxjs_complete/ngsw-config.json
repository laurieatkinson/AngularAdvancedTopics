{
  "index": "/index.html",
  "assetGroups": [{
    "name": "app",
    "installMode": "prefetch",
    "resources": {
      "files": [
        "/favicon.ico",
        "/index.html",
        "/*.css",
        "/*.js"
      ]
    }
  }, {
    "name": "assets",
    "installMode": "lazy",
    "updateMode": "prefetch",
    "resources": {
      "files": [
        "/assets/**"
      ]
    }
  }],
  "dataGroups": [
    {
        "name": "lauriebookservice",
        "urls": ["https://bookservicelaurie.azurewebsites.net/**"],
        "cacheConfig": {
            "maxSize": 11,
            "maxAge": "7d",
            "strategy": "freshness"
        }
    }
]
}